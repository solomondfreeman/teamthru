{
  "name": "teamthru/drupal11-pantheon-starter",
  "description": "Pantheon-ready Drupal 11 site with TeamThru custom theme",
  "type": "project",
  "require": {
    "php": ">=8.3",
    "composer/installers": "^2.2",
    "drupal/core-recommended": "^11.0",
    "drupal/core-composer-scaffold": "^11.0",
    "drupal/core-project-message": "^11.0",
    "drush/drush": "^12.5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true
    }
  }
}
